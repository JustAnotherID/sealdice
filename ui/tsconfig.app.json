{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.tsx",
    "auto-imports.d.ts",
    "components.d.ts",
    "types/index.d.ts"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["ESNext"],
    "paths": {
      "~": ["src"],
      "~/*": ["src/*"],
      "#": ["types"],
      "#/*": ["types/*"]
    },
    "strict": true,
    "target": "ESNext",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo"
  }
}
